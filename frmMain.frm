Private Sub mnuNew_Click()
    If MsgBox("Do you want to start a new game?", vbYesNo, "New Game") = vbNo Then endgame: Exit Sub
        For d = 0 To 76
        Command2(d).Visible = False
        Next
        LRESULT.Caption = ""
    Form_Load
End Sub
